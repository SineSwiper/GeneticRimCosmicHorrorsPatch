<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	<!-- Eldritch Incubators -->



	<RecipeDef>
		<defName>GR_CreateEldritchBearIncubator</defName>
		<label>eldritch bear incubator</label>
		<description>Fill an incubator with genes from a bear and an eldritch beast</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>

			<li>GR_EldritchGenePod</li>				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EldritchGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>EmptyIncubator</li>
				<li>BearGenetic</li>
				<li>GR_EldritchGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_EldritchBearIncubator>1</GR_EldritchBearIncubator>
		</products>
		<researchPrerequisite>GR_EldritchGeneticEngineering</researchPrerequisite>
		<skillRequirements>
			<li>
				<skill>Intellectual</skill>
				<minLevel>8</minLevel>
			</li>
		</skillRequirements>
	</RecipeDef>

